<div class="side-panel" id="bigSlideMenu">
		<a href="javascript: void(0)" class="btn btn-primary slideButton rotateNote" ng-click="showNotes()" id="btn-Notes">
			<i class="fa fa-chevron-up" ng-hide="sliderVisible"></i>
			<i class="fa fa-chevron-down" ng-show="sliderVisible"></i>
			<span>Notes</span>
		</a>
		<a href="javascript: void(0)" class="btn btn-primary slideButton rotateNote" ng-click="showInteractionSlider()" id="btn-QA">
			<i class="fa fa-chevron-up" ng-hide="sliderVisible"></i>
			<i class="fa fa-chevron-down" ng-show="sliderVisible"></i>
			<span>Q&A</span>
		</a>
	<div class="side-panel-body" ng-show="showNoteSlider">
		<% include notesSlider.ejs %>
	</div>
	<div class="side-panel-body" ng-show="showInteractionsSlider" ng-controller="QAInteractionsController">
		<% include ../interactions/qaInteractionSlider.ejs %>
	</div>
</div>