function enableFlips() {
    $('.live-tile').each(function() {
        var id = $(this).attr('id');
        $('#' + id).liveTile({
            repeatCount: 0,
            delay: 0,
            startNow: false,
            mode: 'flip',
            speed: 1000
        });
        $('#' + id).find('.flip_course_click').on("click", function() {
            $('#' + id).liveTile("play", 1);
        });
    });
    
}
$(document).ready(function() {
    $('.sideicon3').show();
    setTimeout(function() {
        $('.number').numeric();
        $('.number').focusout(function() {
            if(this.value > 10 || this.value < 0) {
                this.value = 0;
            } else {
                this.value = parseInt(this.value);
            }
        });
    },1000);
});

function navigateTo(path, param) {
    document.location.href = path + encodeURIComponent(param);
}

function rightBarScrollWidth() {
    if ($('.enscroll-track')) {
        var scrollWidth = $('.enscroll-track').parent().offset().left;
        $('.enscroll-track').parent().css('left', scrollWidth-1);
    }
}

