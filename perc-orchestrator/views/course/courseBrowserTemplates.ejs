<script type="text/ng-template"  id="toc_template.html">
	<div class="unit {{lob.id == currentItem.id ? 'unit'+selectedContentCategory+'ActiveRow'+' unit'+selectedContentCategory+'Active' : ''}} {{lob.id == tocParentId ? 'unitParent' : ''}}" ng-show="lob.level == 0 || (lob.level == 1 && lob.parentId == selectedModule) || (lob.level == 2 && (lob.parentId == selectedLesson || (lob.moduleId == selectedModule && currentItem.level == 0)))" ng-click="setTOCHref(lob.id)" onclick="select(this)" ng-class="{ 'dotted-brd-btm':(lob.isLast && !lob.category)}">
		<div class="row toc_row">
			<span ng-include="'level0_icons_template.html'" class="col-md-1 noPad"></span>
			<span class="col-md-10 noPad toc-lob-name">{{lob.name}}</span>
		</div>
	</div>
</script>

<script type="text/ng-template"  id="level0_icons_template.html">
	<span class="knob" readonly data-progress="{{lob.progress}}">
	<span ng-switch ng-if="lob.category">
		<img style="margin-left: -3px;margin-top: -3px;width:25px;" src="/img/bookmark.png" />
	</span>
	<span ng-switch on="lob.type" ng-if="!lob.category">
		<span class="glyph16 icon icon-moduel" ng-switch-when="module"></span>
		<span class="glyph16 icon icon-lessons" ng-switch-when="lesson"></span>
		<span class="glyph16 icon icon-unorderedlist" ng-switch-when="binder"></span>
		<span class="glyph16 icon icon-Assignment" ng-switch-when="learningactivity"></span>
		<span class="glyph16 icon icon-tutor" ng-switch-when="Tutoring"></span>
		<span class="glyph16 icon icon-tutionclass" ng-switch-when="Classroom"></span>
		<span class="glyph16 icon icon-ref-material" ng-switch-default></span>
	</span>
	</span>
</script>

<script type="text/ng-template"  id="child_list_template.html">
	<div class="panel-heading">
		<div class="panel-icon-cont" style="width: 6%; margin-top: -5px;">
			<span class="knob knobrhs" readonly data-progress="{{child.progress}}">
			<span ng-switch on="child.elementType">
				<span class="glyph16 icon icon-moduel"  ng-switch-when="module"></span>
				<span class="glyph16 icon icon-lessons"  ng-switch-when="lesson"></span>
				<span class="glyph16 icon icon-unorderedlist" ng-switch-when="binder"></span>
				<span class="glyph16 icon icon-Assignment"  ng-switch-when="learningactivity"></span>
				<span class="glyph16 icon-tutor"  ng-switch-when="Tutoring"></span>
				<span class="glyph16 icon icon-tutionclass"  ng-switch-when="Classroom"></span>
				<span class="glyph16 icon icon-ref-material" ng-switch-default></span>
			</span>
			</span>
		</div>
		<div class="panel-header-content">
			<h3 class="panel-title content_title">
				<a ui-sref="cb({lobId:child.identifier})" ng-if="child.elementType == 'module' || child.elementType == 'lesson' || child.elementType == 'binder'" ng-click="scrollTop()">{{child.name}}</a>
				<a ui-sref="cbLecture({lobId:child.parentId, elementId:child.identifier})" ng-if="child.elementType != 'module' && child.elementType != 'lesson' && child.elementType != 'binder'" ng-click="scrollTop()">{{child.name}}</a>
			</h3>
			<div class="video-view-details">
			<span ng-repeat="count in child.elements_count">
				<span ng-switch on="count.elementType">
					<span class="glyph16 icon icon-ref-material"  ng-switch-when="Lectures"></span>
					<span class="glyph16 icon icon-exercise-content"  ng-switch-when="Exercises"></span>
					<span class="glyph16 icon icon-Assignment"  ng-switch-when="Assignments"></span>
					<span class="glyph16 icon-tutor"  ng-switch-when="Tutoring"></span>
					<span class="glyph16 icon icon-tutionclass"  ng-switch-when="Classroom"></span>
					<span class="glyph16 icon icon-tutor" ng-switch-default></span>
				</span>{{count.total}}&emsp;
			</span>
			</div>
			<div ng-show="child.category" class="pull-right">
				<span class="label label-warning background-{{child.category}}">{{child.category}}</span>&emsp;&emsp;
				<a href="javascript:void(0)" class="btn btn-default btn-xs action_box" ng-click="removeAdditionalMaterial(currentObject.identifier, child)" ng-show="child.category">
					<span class="glyphicon glyphicon-trash"></span>
				</a>
			</div>
			<p class="common_content">
				{{child.description | limitTo : 250}}
				<span ng-if="child.description.length > 250">...</span>
			</p>
		</div>
		<a class="collapse-link {{lessonCollapsed ? 'collapsed' : ''}}" ng-show="child.subChildren && child.subChildren.length > 0" ng-click="lessonCollapsed=!lessonCollapsed">
        	<i class="il-collapse"></i>
        </a>
	</div>
	<div collapse="lessonCollapsed" class="panel-collapse collapse">
		<div class="panel-body lectures-container">
			<ul class="ul-style">
				<li ng-repeat="subChild in child.subChildren">
					<span class="knob knobchildrhs" readonly 
					data-progress="{{((subChild.status=='Complete')?'100':(subChild.status=='In Progress')?'50':'0')}}">
					<span ng-switch ng-if="subChild.category">
						<img style="margin-left: -3px;margin-top: -3px;width:25px;" src="/img/bookmark.png" />
					</span>
					<span ng-switch on="subChild.elementType" ng-if="!subChild.category">
						<span class="glyph16 icon icon-moduel"  ng-switch-when="module"></span>
						<span class="glyph16 icon icon-lessons"  ng-switch-when="lesson"></span>
						<span class="glyph16 icon icon-unorderedlist" ng-switch-when="binder"></span>
						<span class="glyph16 icon icon-Assignment"  ng-switch-when="learningactivity"></span>
						<span class="glyph16 icon-tutor"  ng-switch-when="Tutoring"></span>
						<span class="glyph16 icon icon-tutionclass"  ng-switch-when="Classroom"></span>
						<span class="glyph16 icon icon-ref-material" ng-switch-default></span>
					</span>
					</span>
					<div class="child_links">
						<a class="lecture-title" ui-sref="cb({lobId:subChild.identifier})" ng-if="subChild.elementType == 'module' || subChild.elementType == 'lesson' || subChild.elementType == 'binder'" ng-click="scrollTop()">{{subChild.name}}</a>
						<a class="lecture-title" ui-sref="cbLecture({lobId:child.identifier, elementId:subChild.identifier})" ng-if="subChild.elementType != 'module' && subChild.elementType != 'lesson' && subChild.elementType != 'binder'" ng-click="scrollTop()">{{subChild.name}}</a>&emsp;
						<span class="lecture-time" ng-show="subChild.duration" ng-bind-html="renderHtml(subChild.duration)"></span>
						<div class="pull-right" ng-show="subChild.category">
							<span class="label label-warning background-{{subChild.category}}" style="font-size:10px;">{{subChild.category}}</span>
						</div>
					</div>
				</li>
			</ul>
		</div>
	</div>
</script>

