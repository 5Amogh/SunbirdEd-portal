			    	<div class="col-md-6"><label>Concepts</label></div>
			    	<div class="col-md-6" ng-repeat="concept in exam.metadata.concepts" ng-if="exam.metadata.concepts.length > 0">
			        	<span>{{concept}}</span><br/>
			        </div>
			        <div class="col-md-6" ng-if="exam.metadata.concepts.length == 0">None</div>
			    </div>	
			    <div class="col-md-6 noPad">
			        <div class="col-md-6"><label>Difficulty Level</label></div>
			        <div class="col-md-6">{{exam.difficultyLevel.value}}</div>
			        <div ng-if="exam.concepts.length == 0">None</div>
			    </div>
			</div>
</script>


<script type="text/ng-template" id="gradebook_report_details.html">
	<div class="row marginBottom15">
		<div class="col-md-8 QAcontent">
	        <h5 ng-if="report.status=='PROCESSING'">Course Gradebook - New</h5>
	        <h5 ng-if="report.status!='PROCESSING'">Course Gradebook</h5>
	        <div class="detailsQuestion">
	            <ul>
	                <li ng-show="report.createdBy"><span>created by &nbsp;</span><span ng-community-user user-id="report.createdBy" user-name="report.createdName"></span>, <span data-livestamp="{{report.createdDate}}"></span></li>
	                <li ></li>
	            </ul>
	        </div>
	        <div class="clearfix"></div>
	    </div>
	    <div class="col-md-4 QAStatus">
	        <ul class="pull-right">
	            <li class="secondary" ng-show="report.reportInfo.examsCount">
	                <span class="number">{{report.reportInfo.examsCount}}</span>
	                <span>Exams</span>
	            </li>
	            <li class="secondary" ng-show="report.reportInfo.studentsCount">
	                <span class="number">{{report.reportInfo.studentsCount}}</span>
	                <span>Students</span>
	            </li>
	        </ul>
	        <div class="pull-right" style="margin-top: 5px;margin-right: 10px;" ng-switch="report.status">
		    	<div ng-switch-when="COMPLETE"><a href="{{report.reportURL}}" style="font-size: 25px;"><i class="fa fa-download"></i></a></div>
		    	<div ng-switch-when="PROCESSING" style="margin-top: 8px;">Processing...</div>
		    	<div ng-switch-when="NO_DATA" style="margin-top: 8px;">No data available</div>
		    	<div ng-switch-when="FAIL" style="margin-top: 8px;">Gradebook generation failed.</div>
		    </div>
	    </div>
    	<div class="col-md-12"><hr style="margin:0px;"/></div>
    </div>

</script>


