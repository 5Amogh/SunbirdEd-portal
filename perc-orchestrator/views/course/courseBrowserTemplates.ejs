<script type="text/ng-template" id="questions_list_item.html">
    <div class="col-md-12 QAcontent">
        <div class="row">
        	<h5 ng-hide="question.editable" class="col-md-12" style="min-height: 25px">
        		<a href="javascript:void(0);" data-toggle="modal" data-target="#questionPreviewModal" ng-click="getDataForQuestionPreview(question.identifier, 'popupPreviewQuestion')" ng-bind-html="getQuestionTitle(question.body)"></a>
        	</h5>
	        <h5 ng-show="question.editable" class="col-md-10" style="min-height: 25px">
	        	<a href="javascript:void(0);" class="ac-links il-Link-clr questionTitle" ng-bind-html="getQuestionTitle(question.body)" ng-click="editQuestion(question.identifier)"></a>
	        </h5>
	        <div class="col-md-2">
	        	<a href="javascript:void(0);" class="previewIcon marginLeft10 pull-right" ng-click="confirmQuestionToDelete(question)" ng-show="question.editable">
		        	<i class="glyph16 icon icon-delete-round moveDown5" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="Delete question"></i>
		        </a>
		        <a href="javascript:void(0);" class="previewIcon marginLeft10 pull-right" data-toggle="modal" data-target="#questionPreviewModal" ng-click="getDataForQuestionPreview(question.identifier, 'popupPreviewQuestion')" ng-show="question.editable">
	        		<i class="glyph16 fa fa-eye moveDown5" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="Preview question"></i>
	        	</a>
	        </div>
	    </div>
        <div class="detailsQuestion moveTop10">
            <ul class="metadata">
                <li><span>{{question.questionSubtype}}</span></li>
                <li class="list-dash"><span>|</span></li>
                <li><span>{{question.difficultyLevel}}</span></li>
                <li class="list-dash" ng-show="question.testUsage"><span>|</span></li>
                <li  ng-show="question.testUsage"><span>{{question.testUsage}}</span>
                <li class="list-dash" ng-show="question.duration"><span>|</span></li>
                <li  ng-show="question.duration"><span>{{ question.duration | readableTime }}</span>
                </li>
                <li class="list-dash" ng-show="question.learningElements && question.learningElements.length > 0"><span>|</span></li>
                <li ng-show="question.learningElements && question.learningElements.length > 0"><span>In : {{getContentTitleOfItem(question.learningElements);}}</span></li>
            </ul>
        </div>
        <p class="ac-contents" ng-bind-html="question.body"></p>
        <div class="QAtags" ng-show="question.concepts">
            <span class="badge" ng-repeat="concept in question.concepts">{{concept}}</span>
        </div>
        <hr/>
    </div>
</script>

