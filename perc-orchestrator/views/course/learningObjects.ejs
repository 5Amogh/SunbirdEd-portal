<script type="text/ng-template"  id="lob_structure.html">
	<span>
		<i class="icon-minus  exp-col-tree-act" id="icon-{{data.identifier}}" ng-click="treeIconClick($event)"></i>
		<span class="title-txt" id="{{data.identifier}}" ng-show="learningObjectId != data.identifier" ng-click="selectLOB(data)">{{data.name}}</span>
        <span class="title-txt list-active" id="{{data.identifier}}" ng-show="learningObjectId == data.identifier" ng-click="selectLOB(data)">{{data.name}}</span>
	</span>
    <ul ng-show="data.children.length > 0">
        <li ng-repeat="data in data.children | filter:searchText" ng-include="'lob_structure.html'" class="parent_li"></li>
    </ul>
</script>
<script type="text/ng-template"  id="quiz_template.html">
    <div ng-repeat="question in sampleQuiz.questions" ng-show="quizIndex == $index" class="table-responsive">
        <table class="table table-bordered">
            <tbody>
                <tr>
                    <th style="padding:20px;padding-bottom: 10px;">{{question.question}}</th>
                </tr>
                <tr ng-repeat="option in question.options">
                    <td style="padding-left: 20px;"><input type = "radio"> {{option}}</td>
                </tr>
            </tbody>
        </table>
    </div>
    <div style="padding-bottom: 30px;">
        <div class="pull-right">
            <a ng-click="incrementQuizIndex()" ng-show="(quizIndex + 1) < sampleQuiz.questions.length" class="btn btn-sm btn_green">Next</a>
            <a ng-show="(quizIndex + 1) == sampleQuiz.questions.length" class="btn btn-sm btn_green">Finish</a>
        </div>
    </div>
</script>
<body ng-controller="LearningObjectCtrl">
    <% include ../includes/header %>
    <% include ../includes/courseNav %>
	<input type="hidden" id="courseId" value="<%=courseId %>"/>
    <section class="accordion_container">
        <div class="parent_container">
            <div class="collapse_direction showRightPanel">
                <i class="glyphicon glyphicon-chevron-left"></i>
            </div>
            <div class="col-md-2 left-nav-ct left-accrodian" ng-show="addContent != 'show'">
                <div class="search_box">
                    <input type="search" class="form-control search" placeholder="Search" ng-model="searchText"/>
                </div>
                <div class="panel-group" id="accordion">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <h4 class="panel-title">
                                <a data-toggle="collapse" data-parent="#accordion" href="#collapseTwo" class="acc_title" id="toc">
                                    <img class="pull-left" src="/img/prerequisite_icon.png">
                                    <span>Table of Contents</span>
                                </a>
                            </h4>
                        </div>
                        <div id="collapseTwo" class="panel-collapse collapse in">
                            <div class="panel-body">
                                <div class="tree">
                                    <ul class="list-unstyled">
                                        <li ng-repeat="data in courseStructure.children | filter:searchText" ng-include="'lob_structure.html'" class="parent_li"></li>
                                    </ul>
                                </div>
                                <!-- end tree -->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-10 main_container">
                <div class="main">
                    <div class="player-container-">
                        <div class="title">
                            <span>{{selectedTitle}}</span>
                        </div>
                        <div ng-show="selectedResource && learningResourceView != 'addInterceptions'">
                            <div class="sub_title">
                                <span>Name:</span>&nbsp;<span class="desc">{{selectedResource.title}}</span>
                            </div>
                            <div class="video" ng-show="showVideo">
                                <video id="my_video_1" class="video-js vjs-default-skin vjs-big-play-centered"></video>
                            </div>
                            <section ng-include="'quiz_template.html'" ng-show="showQuiz"></section>
                            <div class="sub_title">
                                <span>Description:</span>
                            </div>
                            <p class="desc">
                                <span>{{selectedResource.description}}</span>
                            </p>
                        </div>
                        <div ng-show="selectedResource && learningResourceView == 'addInterceptions'">
                            <div class="sub_title">
                                <span>Name:</span>&nbsp;<span class="desc">{{selectedResource.title}}</span>
                            </div>
                            <div class="video">
                                <video id="video_interception" class="video-js vjs-default-skin vjs-big-play-centered"></video>
                            </div>
                            <div class="form-horizontal" role="form" style="margin-top: 20px">
                                <div class="row">
                                    <div class="form-group col-xs-6">
                                        <label class="col-sm-6 control-label">Start Time {{selectedResource.start}}</label>
                                        <div class="col-sm-6">
                                            <input class="form-control" ng-model="interception['start']">
                                        </div>
                                    </div>
                                    <div class="form-group col-xs-6">
                                        <label class="col-sm-4 control-label">End Time</label>
                                        <div class="col-sm-6">
                                            <input class="form-control" ng-model="interception['end']">
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label">Concept Title</label>
                                        <div class="col-sm-8">
                                            <input class="form-control" ng-model="interception.title">
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                <div class="form-group">
                                    <label class="col-sm-3 control-label">Concept Description</label>
                                    <div class="col-sm-8">
                                        <input class="form-control" ng-model="interception.description">
                                    </div>
                                </div>
                                </div>
                                <div class="row">
                                <div class="form-group">
                                    <label class="col-sm-3 control-label"></label>
                                    <div class="col-sm-8">
                                        <button type="button" ng-click="addPreReq()" class="btn btn-sm btn_green">Add Interception</button>
                                    </div>
                                </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-0 actions">
                <div class="actions_ct" id="actionList">
                    <ul class="selectable">
                         <li>
                            <a href="#"><i class="glyphicon glyphicon-plus-sign"></i> <span>Add</span></a>
                        </li>
                        <li>
                            <a href="#"><i class="glyphicon glyphicon-list-alt"></i> <span>Describe</span></a>
                        </li>
                        <li>
                            <a href="#"><i class="glyphicon glyphicon-export"></i> <span>Enhance</span></a>
                        </li>
                        <li>
                            <a href="#"><i class="glyphicon glyphicon-globe"></i> <span>Enrich</span></a>
                        </li>
                        <li >
