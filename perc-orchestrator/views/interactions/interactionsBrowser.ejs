<!DOCTYPE html>
<html lang="en" ng-app="interactionsApp">
<head>
	<meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="apple-mobile-web-app-capable" content="yes">
  	<meta name="apple-touch-fullscreen" content="yes">
  	<meta id="extViewportMeta" name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0, user-scalable=no">
    <link rel="shortcut icon" href="images/favicon.png">
	<title>SunBird</title>

	<script type="text/javascript" src="https://fast.fonts.net/jsapi/6bb78b32-9b12-4a14-9179-8de98470e4b1.js"></script>

    <!-- Bootstrap -->
    <link rel="stylesheet" href="/css/bootstrap.min.css"/>
    <link rel="stylesheet" href="/css/app.css" />
    <link rel="stylesheet" href="/css/common.css" type="text/css"/>
    <link rel="stylesheet" href="/css/custom.css" type="text/css"/>
	<link rel="stylesheet" href="/css/font-style.css" type="text/css"/>
	<link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css"/>
	<link rel="stylesheet" href="/css/leftNavigation.css" type="text/css"/>
	<link rel="stylesheet" href="/css/interactions.css" type="text/css"/>
	<link rel="stylesheet" href="/css/Markdown.css" type="text/css"/>
	<link rel="stylesheet" href="/css/default-highlight.css" type="text/css"/>

    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body id="courseBrowserBody" class="module-body" ng-controller="InteractionsController" ng-cloak class="ng-cloak">
<div class="wrapper">
<input type="hidden" id="courseId" value="<%=courseId%>"/>
<% include ../includes/header %>
<section class="wrap push container-section container-section-{{selectedCategory}}">
	<section class="sub_section"></section>
	<div class="container">
		<div class="row">
			<div id="tocColumn" class="col-md-3 left-column-ct no-pad-right animatedDiv browserLeftColumn" >
				<div id="tocFloatMenu" ng-show="contentLoaded" class="content-loaded">
					<div id="leftBarHeader" class="il-leftnav">
					  	<ul class="ul-style">
					    	<li>
					    		<a class="tab_heading active pull-left"><span>Q&A</span></a>
					    	</li>
					   </ul>
					</div>
					<div style="border: 1px solid #ddd; border-right: 0px; border-bottom: 0px;">
						<div id="tocParentDiv" class="parent_name">
							<a href="javascript:void(0)" ng-show="currentItem.parent && currentItem.parent.id" ng-click="setTOCHref(currentItem.parent.type, currentItem.parent.id, currentItem.parentId)">
								<div style="width: 10%; float:left"><span ng-switch on="currentItem.parent.type">
									<span class="glyph24 icon icon-course" ng-switch-when="course"></span>
									<span class="glyph24 icon icon-moduel" ng-switch-when="module"></span>
									<span class="glyph24 icon icon-lessons" ng-switch-when="lesson"></span>
									</span>
								</div>
								<div style="width: 90%">{{currentItem.parent.name}}</a></div>
							<div ng-show="!currentItem.parent || !currentItem.parent.id">
								<div style="width: 10%; float:left"><span class="glyph24 icon icon-course"></span></div>
								<div style="width: 90%"><span>{{course.name}}</span></div>
							</div>
						</div>
						<div class="jcarousel tocCarousel" >
							<ul class="ul-style">
								<li id="carousel0" class="il-left-content">
									<div ng-repeat="lob in modules" class="modules">
										<div ng-include="'toc_template.html'"></div>
									</div>
								</li>
								<li id="carousel1" class="il-left-content">
									<div ng-repeat="lob in lessons" class="lessons">
										<div ng-include="'toc_template.html'"></div>
									</div>
								</li>
								<li id="carousel2" class="il-left-content">
									<div ng-repeat="lob in lectures" class="lectures">
										<div ng-include="'toc_template.html'"></div>
									</div>
								</li>
							</ul>
						</div>
						<div id="paginationBtns" class="jcarousel-pagination tocPagination">
							<a id="slide0" href="javascript:void(0)" data-toggle="popover" data-content="Modules" ng-click="showLevel0 ? scrollSlide(0) : void(0)" ng-class="{'disabled' : !showLevel0}"></a>&emsp;
							<a id="slide1" href="javascript:void(0)" data-toggle="popover" data-content="Lessons" ng-click="showLevel1 ? scrollSlide(1) : void(0)" ng-class="{'disabled' : !showLevel1}"></a>&emsp;
							<a id="slide2" href="javascript:void(0)" data-toggle="popover" data-content="Lectures" ng-click="showLevel2 ? scrollSlide(2) : void(0)" ng-class="{'disabled' : !showLevel2}"></a>
						</div>
					</div>
				</div>
			</div>
			<div id="mainContentColumn" ng-show="contentLoaded" class="col-md-9 content-loaded animatedDiv intro-container right-column-ct browserMainColumn" style="min-height: 550px">
				<div>
				    <section>
				    	<div class="il-introContent">
				            <h3><span>{{course.name}}</span></h3>
				            <h1 class="stickyHeader borderBottom">{{learningElementName}}
				            <span class="int-full-icon"><i class="fa fa-book" ng-click="backToCourse()"></i>&nbsp;&nbsp;<i class="fa fa-comments-o"></i></span>
				            </h1>
				        </div>
				    </section>
				</div>
				<div class="interactions-full" ng-view ng-cloak></div>
			</div>
		</div>
	</div>
</section>
</div>
</body>

<script src="/js/jquery.min.js"></script>
<script src="/js/jquery.jcarousel.min.js"></script>
<script src="/js/angular.min.js"></script>
<script src="/js/angular-route.min.js"></script>
<script src="/js/controllers/interactions/interactionsBrowser.js"></script>
<script src="/js/controllers/loginController.js"></script>

<!-- Interactions - controller and directives - Start -->
<script src="/js/controllers/interactions/qaController.js"></script>
<script src="/js/directives/rating-directive.js"></script>
<script src="/js/directives/flag-directive.js"></script>
<script src="/js/directives/follow-directive.js"></script>
<script src="/js/directives/post-comment-directive.js"></script>
<script src="/js/directives/comment-directive.js"></script>
<script src="/js/directives/answer-action-directive.js"></script>
<script src="/js/directives/post-directive.js"></script>
<script src="/js/angular-pagedown.js"></script>
<script src="/js/Markdown.Converter-min.js"></script>
<script src="/js/Markdown.Sanitizer-min.js"></script>
<script src="/js/Markdown.Editor-min.js"></script>
<script src="/js/md5.js"></script>
<script src="/js/enc-base64-min.js"></script>
<script src="/js/moment.js"></script>
<script src="/js/livestamp.min.js"></script>
<script src="/js/highlight.pack.js"></script>
<!-- Interactions - controller and directives - End -->

<script src="/js/bootstrap.min.js"></script>
<script src="/js/application/courseBrowserScripts.js"></script>

<!-- Include the courseBrowser templates -->
<% include ../course/courseBrowserTemplates.ejs %>
<footer id="pageFooter" ng-show="contentLoaded" class="content-loaded">
    <% include ../includes/footer %>
</footer>
</html>