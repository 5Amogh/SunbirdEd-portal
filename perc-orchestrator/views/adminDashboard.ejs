<body id="module-body" ng-controller="DashboardCtrl" ng-cloak class="ng-cloak">
    <header class="nav-down">
        <% include ./includes/header %>
    </header>
    <section class="mid-container-full" ng-cloak>
        <div class="mid-container" style="min-height: 450px">
            <div class="col-sm-1 NavContentz leftbarshadow">
            </div>
            <div class="middleContainer">
                <div class="mid-area scrollstyle" ng-cloak>
                    <div class="mid_content" ng-show="contentLoaded">
                        <div class="col-md-12" ng-hide="showCourse || showCourseSettings">
                            <div><% include ./admin/dashboardAllCourses.html %></div>
                        </div>
                        <div class="col-md-12" ng-show="showCourse">
                            <div><% include ./admin/dashboardViewCourse.html %></div>
                        </div>
                        <div class="col-md-12" ng-show="showCourseSettings">
                            <div><% include ./admin/dashboardCourseSettings.html %></div>
                        </div>
                    </div>
                    <% include ./includes/footer %>
                </div>
            </div>
            <div class="RightSideBar">
                <div class="sidebarContainerz">
                </div>
            </div>
        </div>
    </section>
</body>
<script src="/js/controllers/adminDashboardController.js"></script>
<script src="/js/controllers/coachDashboardController.js"></script>
<script src="/js/controllers/loginController.js"></script>
