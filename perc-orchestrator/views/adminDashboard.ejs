<!-- <script src="/js/custom.js"></script> -->
<script src="/js/angular-sanitize.min.js"></script>
<script src="/js/bootstrap-filestyle.min.js"></script>
<script src="/js/jquery.numeric.min.js"></script>

<script>

$(document).ready(function() {
    setTimeout(function() {
        $('.number').numeric();
        $('.number').focusout(function() {
            if(this.value > 10 || this.value < 0) {
                this.value = 0;
            } else {
                this.value = parseInt(this.value);
            }
        });
    },1000);
});

function navigateTo(path, param) {
    document.location.href = path + encodeURIComponent(param);
}

function windowHeight1() {
    var windowH = $(window).outerHeight();
    var headerH = $("header").outerHeight();
    var footerH = $("footer").outerHeight();
    $(".RightSideBar").height(windowH - headerH);
    $(".mid-area").height(windowH - headerH);
    $(".customHeight").height(windowH - headerH);
    $(".NavLeftContainer:first-child ul").height(windowH - headerH);
    //$(".video-container iframe,.video-container").height(windowH - 350 );

}

setTimeout(function() {
    windowHeight1();
}, 500);

</script>

</html>
