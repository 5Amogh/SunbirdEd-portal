<body class="home-body" id="home-body" ng-controller="TermsAndConditionsCtrl">
    <header class="nav-down">
        <% include ../includes/header %>
    </header>
    <section class="mid-container-full">
        
        <div class="mid-container" >
            
            <div class="middleContainer">
                <div class="mid-area scrollstyle noPad" ng-cloak>
                    <div class="mid_content">
                        <div class="List_view ng-cloak" ng-cloak>
                            <div class=" askquestionHeader">
                                <div class="headerAsk">
                                    <h2>Terms of use</h2>
                                </div>
                                <p style="font-weight:bold;">Welcome</p>
                                <p>Please accept the terms of use and honour code to proceed further.</p>
                                <div class="il-web-padng" style="padding-top:0px;padding-bottom: 5px;">
                                    <div class="row container noPad il_explore_content">
                                        <div class="col-md-12" style="height: 350px; overflow-y: auto; display: block; opacity: 1; border: 1px solid #ededed;">
                                            <% include studentReadiness.html %>
                                        </div>
                                        <label style="padding-top:10px; font-weight: normal;">
                                            <input type="checkbox" ng-model="termsofuse">
                                            &nbsp;&nbsp;I accept the <a ng-click="moveToLocation('termsofuse')">terms of use</a>.
                                        </label><br/>
                                        <label style="font-weight: normal;">
                                            <input type="checkbox" ng-model="honorcode">
                                            &nbsp;&nbsp;I accept the <a ng-click="moveToLocation('honorcode')">honor code</a>.
                                        </label>
                                        <div class="alert alert-danger" ng-show="tandcsubmitted && (!termsofuse || !honorcode)">
                                            <span>In order to use our services, you must agree to our terms of use and honor code.</span>
                                        </div>
                                        <div style="padding-top:10px; border-bottom: 1px solid #ededed;padding-bottom:40px">
                                            <button class="btn btn-primary" ng-click="continue()">continue</button>
                                        </div>
                                    <br/>
                                    </div>
                                </div>
                        </div>
                    </div>

                    </div>
                    <% include ../includes/footer %>
                </div>
            </div>
            
        </div>
    </section>
    
