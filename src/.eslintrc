{
    "extends": "standard",
    "env": {
        "browser": true,
        "node": true,
        "jasmine": true,
        "mocha": true
    },
    "globals": {
        "angular": true,
        "_": true,
        "$": true,
        "moment": true,
        "iziToast": true,
        "org": true,
        "alasql": true,
        "md5": true
    },
    "rules": {
        "quotes": ["error", "single"],
        "max-len": ["error", 120, 4, {"ignoreUrls": true}],
        "indent": ["error", 2]
    }
}
